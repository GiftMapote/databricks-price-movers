{
  "datasets": [
    {
      "name": "b7d4e018",
      "displayName": "Hourly Trend",
      "queryLines": [
        "SELECT\n",
        "  hour_ts_sa,\n",
        "  asset_id,\n",
        "  vs_currency,\n",
        "  avg_price,\n",
        "  samples\n",
        "FROM gold_hourly_prices\n",
        "WHERE hour_ts >= now() - INTERVAL 24 HOURS\n",
        "ORDER BY hour_ts_sa;\n"
      ],
      "catalog": "databricks_cata",
      "schema": "price_movers"
    },
    {
      "name": "8c4b59d4",
      "displayName": "Top Movers (Latest Hour)",
      "queryLines": [
        "WITH latest_hour AS (\n",
        "  SELECT MAX(hour_ts_sa) AS max_hour\n",
        "  FROM gold_hourly_movers\n",
        ")\n",
        "SELECT\n",
        "  m.asset_id,\n",
        "  m.vs_currency,\n",
        "  m.hour_ts_sa,\n",
        "  m.avg_price,\n",
        "  m.prev_avg_price,\n",
        "  m.pct_change,\n",
        "  samples\n",
        "FROM gold_hourly_movers m\n",
        "JOIN latest_hour h\n",
        "  ON m.hour_ts_sa = h.max_hour\n",
        "ORDER BY m.pct_change DESC;\n"
      ],
      "catalog": "databricks_cata",
      "schema": "price_movers"
    }
  ],
